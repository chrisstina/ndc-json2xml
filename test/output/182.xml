<IATA_AirShoppingRQ
        xsi:schemaLocation="http://www.iata.org/IATA/2015/00/2018.2/IATA_AirShoppingRQ"
        xmlns="http://www.iata.org/IATA/2015/00/2018.2/IATA_AirShoppingRQ"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Party>
        <Sender>
            <TravelAgency>
                <AgencyID>1234</AgencyID>
            </TravelAgency>
        </Sender>
    </Party>
    <PayloadAttributes>
        <PrimaryLangID>RU</PrimaryLangID>
        <VersionNumber>18.2</VersionNumber>
    </PayloadAttributes>
    <PointOfSale>
        <Country>
            <CountryCode>RU</CountryCode>
        </Country>
    </PointOfSale>
    <Request>
        <FlightCriteria>
            <OriginDestCriteria>
                <DestArrivalCriteria>
                    <IATALocationCode>MOW</IATALocationCode>
                </DestArrivalCriteria>
                <OriginDepCriteria>
                    <Date>2021-03-20</Date>
                    <IATALocationCode>LED</IATALocationCode>
                </OriginDepCriteria>
            </OriginDestCriteria>
        </FlightCriteria>
        <Paxs>
            <Pax>
                <PaxID>1</PaxID>
                <PTC>ADT</PTC>
            </Pax>
        </Paxs>
        <ShoppingCriteria>
            <CabinTypeCriteria>
                <CabinTypeCode>3</CabinTypeCode>
            </CabinTypeCriteria>
            <CarrierCriteria>
                <Carrier>
                    <AirlineDesigCode>SU</AirlineDesigCode>
                </Carrier>
            </CarrierCriteria>
            <SpecialNeedsCriteria>
                <Qty>50</Qty>
            </SpecialNeedsCriteria>
        </ShoppingCriteria>
    </Request>
</IATA_AirShoppingRQ>